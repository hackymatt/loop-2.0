[run]
branch = true
omit = 
    */csrf/*
    */tests/*
    */utils/*
    */migrations/*
    */settings/*
    */cron.py
    */asgi.py
    */wsgi.py
    */gunicorn.py
    manage.py
source = .

[report]
fail_under = 100
show_missing = True
skip_empty = True
exclude_lines =
    pragma: no cover