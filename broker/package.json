{
  "name": "broker",
  "version": "1.0.0",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "lint": "eslint \"src/**/*.{js,ts}\"",
    "lint:check": "eslint \"src/**/*.{js,ts}\" --max-warnings=0",
    "lint:fix": "eslint --fix \"src/**/*.{js,ts}\"",
    "lint:print": "npx eslint --print-config eslint.config.mjs > eslint-show-config.json",
    "fm:check": "prettier --check \"src/**/*.{js,ts}\"",
    "fm:fix": "prettier --write \"src/**/*.{js,ts}\"",
    "fix:all": "npm run lint:fix && npm run fm:fix",
    "test": "mocha --require ts-node/register tests/**/*.test.ts",
    "test:coverage": "nyc --check-coverage --lines 100 --branches 100 --functions 100 --statements 100 npm run test"
  },
  "engines": {
    "node": "20.x"
  },
  "dependencies": {
    "@kubernetes/client-node": "^0.22.3",
    "amqplib": "^0.10.8",
    "body-parser": "^1.20.3",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@types/amqplib": "^0.10.7",
    "@types/body-parser": "^1.19.2",
    "@types/chai": "^4.2.2",
    "@types/express": "^5.0.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.15.17",
    "@types/proxyquire": "^1.3.31",
    "@types/sinon": "^17.0.4",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/parser": "^8.17.0",
    "chai": "^4.5.0",
    "eslint": "^9.16.0",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-perfectionist": "^4.2.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "mocha": "^11.2.2",
    "nyc": "^17.1.0",
    "prettier": "^3.4.2",
    "proxyquire": "^2.1.3",
    "sinon": "^20.0.0",
    "supertest": "^7.1.1",
    "ts-mocha": "^11.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.17.0"
  }
}
